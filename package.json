{
  "name": "experimental-websocket-server",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/shaunanoordin/experimental-websocket-server/"
  },
  "homepage": "https://github.com/shaunanoordin/experimental-websocket-server/",
  "babel": {
    "presets": ["es2015"]
  },
  "dependencies": {
    "express": "4.13.4",
    "concurrently": "2.0.0",
    "webpack": "1.13.1",
    
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4", 
    "babel-preset-es2015": "6.9.0",
    
    "stylus": "0.54.5"
  },
  "scripts": {
    "start": "node server.js",
    "build": "webpack && stylus src -o app",
    "dev": "concurrently \"webpack --watch\" \"stylus src -o app --watch\" \"node server.js\""
  },
  "engines": {
    "node": "6.2.0"
  }
}
